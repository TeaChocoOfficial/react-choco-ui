{
    "name": "@teachoco-dev/react-choco-ui",
    "version": "0.0.1",
    "type": "module",
    "private": false,
    "license": "ISC",
    "author": "teachoco-official",
    "description": "javascript npm library create by teachoco-official",
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.cjs.js"
        },
        "./vite": "./dist/config/vite.js",
        "./styles.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/@teachoco-official/react-choco-ui"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TeaChocoOfficial/react-choco-ui"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "public": "npm publish --scope=@teachoco-official --access=public",
        "publicforce": "npm publish --scope=@teachoco-official --access=public --force"
    },
    "peerDependencies": {
        "@tailwindcss/vite": "^4.1.16",
        "react": "^19.2.0",
        "react-dom": "^19.2.0"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.16",
        "react": "^19.2.0",
        "react-dom": "^19.2.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.0",
        "@storybook/addon-docs": "10.0.2",
        "@storybook/addon-onboarding": "10.0.2",
        "@storybook/react-vite": "10.0.2",
        "@types/node": "^24.9.2",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@vitejs/plugin-react": "^5.1.0",
        "autoprefixer": "^10.4.21",
        "babel-plugin-react-compiler": "19.1.0-rc.3",
        "eslint": "^9.39.0",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "eslint-plugin-storybook": "10.0.2",
        "globals": "^16.5.0",
        "postcss": "^8.5.6",
        "storybook": "^10.0.2",
        "tailwindcss": "^4.1.16",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.46.2",
